if [[ $DG_USER_LEVEL > 1 ]]; then
  less_flags=" ${DG_ANSI_BG_MAG}-FX${DG_ANSI_X}${DG_ANSI_INVERT}"
  ls_flags1=" ${DG_ANSI_BG_MAG}-1${DG_ANSI_X}${DG_ANSI_INVERT}"
  ls_flags2="${DG_ANSI_BG_MAG}-1${DG_ANSI_X}${DG_ANSI_INVERT}"
else
  less_flags=""
  ls_flags1=""
  ls_flags2="-"
fi
